var mark = 95;
var result = '';


if (mark >=0 && mark <= 49)
result = 'Fail';

else if (mark >=50 && mark <= 64)
result = 'Pass';

else if (mark >=65 && mark <= 74)
result = 'Credit';

else if (mark >=75 && mark <= 84)
result = 'Distinction';

else if (mark >=85 && mark <= 100)
result = 'High Distinction';

else 
result = 'Unidentified';

gs.info('Student Scored: ' + mark + ' '+'Student Result: ' + result)

